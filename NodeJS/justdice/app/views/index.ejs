<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>My Node.js Project</title>
</head>

<body>
    <h1>User List</h1>
    <ul>
        <% users.forEach(user=> { %>
            <li>
                <%= user.name %>
            </li>
            <% }) %>
    </ul>

    <script src="/socket.io/socket.io.js"></script>
    <script>
        const socket = io();
        socket.emit('message', 'Hello from client!');
    </script>
</body>

</html>